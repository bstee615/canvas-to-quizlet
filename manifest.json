  {
    "name": "Canvas to Quizlet",
    "version": "1.0",
    "description": "Converts a Canvas quiz page to Quizlet questions.",
    "manifest_version": 2,
    "icons": { "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png" },

	"background": {
		"scripts": ["popup.js"],
		"persistent": true
	},
	"permissions": [
        "tabs",
        "http://*.instructure.com/",
        "https://*.instructure.com/"
    ],
	"browser_action": {
		"default_logo": "logo128.png",
		"default_popup": "popup.html"
    }
  }